

<div class="page-header">
  <h1>Listing Daycare Providers</h1>
</div>


<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>Location name</th>
        <th>Address</th>
        <th>City</th>
        <th>State</th>
        <th>Zip</th>
        <th>Phone</th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @providers) do |provider| %>
        <td><%= link_to provider.Location_Name, provider %></td>
        <td><%= provider.Address %></td>
        <td><%= provider.City %></td>
        <td><%= provider.State %></td>
        <td><%= provider.Zip %></td>
        <td><%= provider.Phone %></td>
      <% end %>
    </tbody>
  </table>
</div>

